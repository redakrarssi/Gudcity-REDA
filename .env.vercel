# Public config for frontend (safe to expose)
NODE_ENV=production
VITE_APP_ENV=production

# API endpoints (public)
VITE_API_URL=https://api.vcarda.com
APP_URL=https://www.vcarda.com

# Security (public)
VITE_QR_ENCRYPTION_ENABLED=true
VITE_CORS_ORIGINS=https://www.vcarda.com,https://api.vcarda.com
VITE_CSP_ENABLED=true

# Feature toggles
VITE_ENABLE_FEEDBACK=true
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_SHOW_MOCK_NOTICE=false
VITE_MOCK_AUTH=false
VITE_MOCK_DATA=false
