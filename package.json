{
  "name": "gudcity-loyalty-platform",
  "private": true,
  "version": "10.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "node dev-server.js",
    "build": "vite build && node scripts/patch-vendor-charts.js",
    "build:vercel": "node scripts/vercel-build.js",
    "postbuild": "node scripts/disable-sound.js",
    "build:analyze": "ANALYZE=true vite build",
    "build:prod": "vite build --mode production && node scripts/patch-vendor-charts.js",
    "patch-charts": "node scripts/patch-vendor-charts.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch --config=jest.config.cjs",
    "test:coverage": "jest --coverage --config=jest.config.cjs",
    "serve:prod": "vite preview --port 5173",
    "lighthouse": "lighthouse http://localhost:5173 --view",
    "server": "node --experimental-specifier-resolution=node dist/server.js",
    "dev:server": "nodemon --exec ts-node-esm src/server.ts",
    "api:server": "node start-api-server.cjs",
    "api:fix": "node src/server-fix.mjs",
    "api:debug": "node --inspect start-api-server.cjs",
    "type-check": "tsc --noEmit",
    "type-safety": "node scripts/run-type-safety-completion.js",
    "type-safety:dry": "node scripts/run-type-safety-completion.js --dry-run",
    "type-optimize": "node scripts/optimize-type-performance.js",
    "type-codemod": "node scripts/type-safety-codemod.js",
    "type-complete": "node scripts/complete-type-safety.js",
    "context7": "npx -y @upstash/context7-mcp",
    "security:test": "node scripts/test-security.mjs",
    "security:validate": "node -e \"import('./src/utils/validateEnvironment.js').then(m => m.logSecurityValidation())\"",
    "security:grep": "node scripts/security-grep.mjs",
    "security:check": "npm run security:validate && npm run security:test && npm run security:grep",
    "security:audit": "npm audit && npm run security:check",
    "predeploy": "npm run security:check",
    "admin:init-db": "node scripts/init-admin-db.mjs",
    "admin:check-health": "node scripts/check-admin-db-health.mjs",
    "admin:fix-passwords": "node scripts/fix-password-update.mjs",
    "admin:api-server": "node start-api-server.cjs",
    "security:generate-secrets": "node scripts/generate-secure-secrets.js",
    "security:setup-production": "node scripts/setup-production-security.js",
    "security:validate-production": "node scripts/validate-security.js",
    "security:production-check": "npm run security:generate-secrets && npm run security:setup-production",
    "migrate:security": "node db/run-security-migrations.mjs",
    "db:migrate": "node db/run-security-migrations.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@neondatabase/serverless": "^1.0.1",
    "@tanstack/react-query": "^5.80.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/mapbox-gl": "^3.4.1",
    "@types/qrcode": "^1.5.5",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "framer-motion": "^12.18.1",
    "glob": "^11.0.2",
    "helmet": "^8.1.0",
    "html5-qrcode": "^2.3.8",
    "i18next": "^23.10.1",
    "jsonwebtoken": "^9.0.2",
    "limiter": "^3.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "mapbox-gl": "^3.12.0",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "postgres-pool": "^10.1.12",
    "qrcode": "^1.5.4",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.15.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet-async": "^1.0.1",
    "@types/react-i18next": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@upstash/context7-mcp": "^1.0.14",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "canvas": "^3.1.0",
    "compression-webpack-plugin": "^11.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "lighthouse": "^12.8.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.40.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.3.5",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "overrides": {
    "react-qr-reader": {
      "react": "$react"
    }
  }
}
