# Example Environment File for GudCity Platform
# Copy to .env.secure for backend secrets
# Copy to .env.vercel for frontend deployment

# Database
DATABASE_URL=postgresql://username:password@hostname:5432/dbname?sslmode=require

# JWT secrets
JWT_SECRET=replace_with_secure_secret
JWT_REFRESH_SECRET=replace_with_secure_refresh_secret

# QR secrets
QR_SECRET_KEY=replace_with_secure_key
QR_ENCRYPTION_KEY=replace_with_secure_key

# API
VITE_API_URL=http://localhost:3000
APP_URL=http://localhost:5173
